{
  "name": "cocos-creator-inspector",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "npm run lint",
    "lint": "eslint --ext js,vue --max-warnings 0 src",
    "clean": "rm -rf build",
    "build": "webpack --mode production -p --progress",
    "build:clean": "npm run clean && webpack --mode production -p",
    "build:watch": "webpack --mode development -w --display none"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.11.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "element-ui": "^2.13.0",
    "vue": "^2.5.13"
  }
}
